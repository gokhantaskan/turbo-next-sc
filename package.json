{
  "name": "my-turborepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "start": "turbo start",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "lint:root": "eslint \"*.{js,cjs,mjs,ts}\" --fix",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json,svg}\"  -l -u",
    "lf": "pnpm lint:fix && pnpm format",
    "type-check": "turbo type-check",
    "clean": "rimraf --glob **/dist **/.next **/next-env.d.ts **/.turbo **/.output **/node_modules/.vite **/node_modules/.cache **/inspect",
    "clean:all": "pnpm clean && rimraf --glob ./**/node_modules"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.1.2",
    "typescript": "^5.6.2"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=20"
  }
}
